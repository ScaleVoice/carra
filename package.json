{
  "name": "carra",
  "version": "0.1.0",
  "scripts": {
    "reset": "npx rimraf ./**/node_modules",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "svgr:generate": "svgr ./src/icons --config-file svgr-cli.config.js",
    "codegen": "swagger-codegen generate -i https://api.carra.devel.cleevio.dev/api/api-docs -l typescript-fetch -o ./swagger",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "engines": {
    "node": ">=16.18.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/",
    "src/icons"
  ],
  "dependencies": {
    "@clerk/nextjs": "^4.29.7",
    "@headlessui/react": "^1.7.18",
    "@headlessui/tailwindcss": "^0.2.0",
    "@radix-ui/react-popover": "^1.0.7",
    "@sentry/nextjs": "^7.77.0",
    "@svgr/cli": "^8.1.0",
    "@tanstack/react-query": "^5.22.2",
    "@tanstack/react-query-next-experimental": "^5.22.2",
    "@tanstack/react-table": "^8.10.3",
    "class-variance-authority": "^0.7.0",
    "date-fns": "^3.3.1",
    "firebase": "^10.4.0",
    "generate-ts-from-env": "^1.0.5",
    "i18next": "^23.5.1",
    "i18next-resources-to-backend": "^1.2.0",
    "immer": "^10.0.3",
    "luxon": "^3.4.3",
    "next": "^13.5.4",
    "next-i18n-router": "^5.3.1",
    "react": "~18.2.0",
    "react-datepicker": "^6.2.0",
    "react-dom": "~18.2.0",
    "react-hook-form": "^7.46.2",
    "react-i18next": "^13.2.2",
    "react-no-ssr": "^1.1.0",
    "recharts": "^2.12.1",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/nextjs": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@tanstack/eslint-plugin-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^5.24.0",
    "@types/jest": "~29.2.6",
    "@types/luxon": "^3.3.3",
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.2.0",
    "autoprefixer": "^10.4.17",
    "eslint": "8.51.0",
    "eslint-config-next": "13.5.4",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "~8.0.3",
    "lint-staged": "~13.1.4",
    "postcss": "^8.4.35",
    "postcss-import": "^16.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "rimraf": "~3.0.2",
    "storybook": "^7.5.3",
    "storybook-addon-pseudo-states": "^2.1.2",
    "tailwindcss": "~3.3.3",
    "typescript": "~5.0.4"
  }
